<div class="bg-white nm">
  <div class="container py-5">
    <h1>Log in</h1>
    <div class="text-center py-5 my-5">
      <%= form_with(scope: :sessions, url: login_path, local: true) do |f| %>
        <p>
          <%= f.label :Username %><br />
          <%= f.text_field :username %>
        </p>
        <p>
          <%= f.submit "Login"%>
        </p>
      <% end %>
    </div>
  </div>
</div>